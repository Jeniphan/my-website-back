{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "env": {
    "DATABASE_URL": "@prisma/db-url"
  },
  "functions": {
    "prisma": {
      "memory": 512,
      "runtime": "nodejs14.x",
      "environment": {
        "PRISMA_QUERY_ENGINE_BINARY": "./node_modules/.prisma/client/query-engine-linux-arm"
      }
    }
  }
}